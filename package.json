  "browserslist": [
    "Chrome >= 70",
    "Safari >= 13.1",
    "iOS >= 13.4",
    "Firefox >= 69",
    "Edge >= 79",
    "Samsung >= 12"
  ],
  "nano-staged": {
    "src/**/*.{ts,tsx}": "eslint --cache --fix",
    "src/**/*.css": "stylelint --fix",
    "*.{js,ts,tsx,json,css,html,svg}": "prettier --write"
  }
}
