{
  "name": "capital-ui",
  "version": "1.0.0",
  "description": "Capital UI",
  "author": "Tranwall",
  "license": "ISC",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "lint:ts": "eslint --ext .ts,.tsx src/ --cache --max-warnings 0",
    "lint:css": "stylelint --cache \"src/**/*.css\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "build": "webpack --mode production",
    "analyze": "webpack --mode production --env analyzer=default",
    "start": "webpack-dev-server --mode development --open --hot",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "core-js": "3.18.1",
    "solid-create-form": "0.2.1",
    "solid-js": "1.1.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.3.8",
    "@storybook/addon-essentials": "6.3.8",
    "@storybook/addon-links": "6.3.8",
    "@storybook/builder-webpack5": "6.3.8",
    "@storybook/html": "6.3.8",
    "@storybook/manager-webpack5": "6.3.8",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "@vue/preload-webpack-plugin": "2.0.0",
    "autoprefixer": "10.3.6",
    "babel-loader": "8.2.2",
    "babel-plugin-polyfill-corejs3": "0.2.5",
    "babel-preset-solid": "1.1.5",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.3.0",
    "css-minimizer-webpack-plugin": "3.0.2",
    "eslint": "7.32.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-solid": "0.1.2",
    "fork-ts-checker-webpack-plugin": "6.3.3",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "mini-css-extract-plugin": "2.3.0",
    "postcss": "8.3.8",
    "postcss-loader": "6.1.1",
    "postcss-nesting": "8.0.1",
    "prettier": "2.4.1",
    "style-loader": "3.3.0",
    "stylelint": "13.13.1",
    "stylelint-order": "4.1.0",
    "ts-loader": "9.2.6",
    "typescript": "4.4.3",
    "webpack": "5.54.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.3.0"
  },
  "browserslist": [
    "Chrome >= 70",
    "Safari >= 13.1",
    "iOS >= 13.4",
    "Firefox >= 69",
    "Edge >= 79",
    "Samsung >= 12"
  ],
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --cache --fix",
    "src/**/*.css": "stylelint --fix",
    "*.{js,ts,tsx,json,css,html}": "prettier --write"
  }
}
